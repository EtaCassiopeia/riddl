domain innovapost {
}
domain ngtt subdomain of innovapost {

type SiteId is Id

entity Plant is SiteId

type PackageId is Id
type ContainerId is Id
type TripId is Id

type PackageInfo combine {
  id: PackageId
  containedIn: ContainerId?
}

entity Package is PackageInfo {}

type ContainerInfo is combine {
  id: ContainerId
  nested: ContainerId*
  packages: PackageId*
}

entity Container is ContainerInfo {
}

type VehicleId is Id
type VehicleInfo is combine {
  id: VehicleId
  contains: ContainerId
}

entity Vehicle is VehicleInfo {
}

}

